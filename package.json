{
  "name": "preact-express-boilerplate-no-build",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "description": "",
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "DEBUG=aima-schedule-check:* npm run dev",
    "dev": "concurrently -c \"green,blue\" -n server,tailwind \"nodemon ./bin/www\" \"npm run tailwind:start\"",
    "tailwind:build": "tailwindcss -i tailwind.css -o ./public/css/styles.css",
    "tailwind:start": "npm run tailwind:build -- --minify --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "htm": "^3.1.1",
    "morgan": "~1.9.1",
    "preact": "^10.19.3",
    "preact-async-route": "^2.2.1",
    "preact-router": "^4.1.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "tailwindcss": "^3.4.1"
  }
}
